option_settings:
  aws:elasticbeanstalk:application:environment:
    AWS_REGION: '`{"Ref" : "AWS::Region"}`'
    GOOGLE_SERVICE_ACCOUNT_FILE_PATH: "google-service-account-key.json"
    GOOGLE_SERVICE_ACCOUNT2_FILE_PATH: "google-service-account-key-2.json"
    EVENTS_TABLE: Vorder-Events
    SESSIONS_TABLE: Vorder-Sessions
    CREDENTIALS_TABLE: Vorder-Creds
    EVENTS_BUCKET: vorder-user-recordings
    ORDER_PROCESSING_SCRIPT_PATH: order_processing.py
  aws:elasticbeanstalk:environment:proxy:
    ProxyServer: nginx
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: /static